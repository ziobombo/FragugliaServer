[global_tags]
[agent]
  interval = "10s"
  round_interval = true
  metric_batch_size = 1000
  metric_buffer_limit = 10000
  collection_jitter = "0s"
  flush_interval = "10s"
  flush_jitter = "0s"
  precision = ""
  hostname = ""
  omit_hostname = false
[[outputs.influxdb]]
[[inputs.cpu]]
  percpu = false
  totalcpu = true
  collect_cpu_time = false
  report_active = false
[[inputs.disk]]
  ignore_fs = ["tmpfs", "devtmpfs", "devfs", "iso9660", "overlay", "aufs", "squashfs"]
[[inputs.diskio]]
  skip_serial_number = false
  device_tags = ["ID_SERIAL"]
[[inputs.zfs]]
  poolMetrics = true
[[inputs.net]]
[[inputs.mem]]
[[inputs.processes]]
[[inputs.swap]]
[[inputs.system]]
[[inputs.ping]]
  interval = "60s"
  urls = ["8.8.8.8","8.8.4.4"]
  method = "exec"
[[inputs.exec]]
  interval = "3600s"
  commands = ["/usr/bin/speedtest-cli --json"]
  timeout = "60s"
  data_format = "json"
  name_suffix = "_speedtest"