[global_tags]
[agent]
  interval = "10s"
  round_interval = true
  metric_batch_size = 1000
  metric_buffer_limit = 10000
  collection_jitter = "0s"
  flush_interval = "10s"
  flush_jitter = "0s"
  precision = ""
  hostname = "{{influxdb_hostname}}"
  omit_hostname = false
  logfile = "/var/log/telegraf/telegraf.log"
[[outputs.influxdb_v2]]
  urls = ["https://metrics.fraguglia.it"]
  token = "{{server0_token}}"
  organization = "fraguglia.it"
  bucket = "base"
[[inputs.cpu]]
  percpu = false
  totalcpu = true
  collect_cpu_time = false
  report_active = false
[[inputs.disk]]
  interval = "1m"
  ignore_fs = ["tmpfs", "devtmpfs", "devfs", "iso9660", "overlay", "aufs", "squashfs"]
[[inputs.diskio]]
  skip_serial_number = false
  device_tags = ["ID_SERIAL"]
[[inputs.net]]
[[inputs.mem]]
[[inputs.swap]]
[[inputs.system]]
